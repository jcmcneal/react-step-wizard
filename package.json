{
    "name": "react-step-wizard",
    "version": "5.4.0-beta.0",
    "description": "A modern flexible step wizard component built for React",
    "types": "dist/index.d.ts",
    "main": "dist/react-step-wizard.cjs.js",
    "browser": "dist/react-step-wizard.umd.js",
    "module": "dist/react-step-wizard.esm.js",
    "scripts": {
        "start": "export NODE_ENV=development; ./scripts/run.dev.js",
        "dev": "rollup --config rollup.config.js --watch --bundleConfigAsCjs",
        "build": "rollup --config rollup.config.js --environment NODE_ENV:production --bundleConfigAsCjs && npm run build:types",
        "build:types": "tsc --declaration --emitDeclarationOnly --outFile ./dist/index.d.ts",
        "app:dev": "webpack-dev-server --open --mode='development' --hotOnly --config app/webpack.config.js",
        "app:build": "export NODE_ENV=production; webpack --progress --optimize-minimize --mode=production --config app/webpack.config.js",
        "prepublishOnly": "rm -rf ./dist && npm run build",
        "test": "jest --config=jest.config.js",
        "lint": "eslint src/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jcmcneal/react-step-wizard.git"
    },
    "keywords": [
        "react",
        "step",
        "multistep",
        "wizard",
        "es6"
    ],
    "author": "Jason McNeal <jaywizard@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jcmcneal/react-step-wizard/issues"
    },
    "homepage": "https://github.com/jcmcneal/react-step-wizard#readme",
    "peerDependencies": {
        "react": ">=15.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@rollup/plugin-eslint": "^9.0.5",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.14.10",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@typescript-eslint/parser": "^7.16.0",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "bootstrap": "^4.4.1",
        "css-loader": "^3.4.2",
        "docsify-cli": "^4.4.4",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-react": "^7.34.4",
        "identity-obj-proxy": "^3.0.0",
        "iso-morphic-style-loader": "^2.0.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "less": "^3.13.1",
        "less-loader": "^5.0.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hot-loader": "^4.12.19",
        "react-test-renderer": "^18.3.1",
        "rollup": "^4.18.1",
        "rollup-plugin-filesize": "^10.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-uglify": "^5.0.2",
        "terser-webpack-plugin": "^2.3.5",
        "ts-jest": "^29.2.2",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "tslib": "^2.6.3",
        "typescript": "^5.5.3",
        "typescript-plugin-css-modules": "^5.1.0",
        "webpack": "^4.41.6",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.2"
    }
}
